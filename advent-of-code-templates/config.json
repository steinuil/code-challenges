{
  "extension": {
    "erlang": "erl",
    "fsharp": "fsx",
    "ruby": "rb"
  },
  "runner": {
    "ruby": {
      "command": ["ruby", "<SCRIPT>", "<INPUT>"],
      "change_directory": false
    },
    "fsharp": {
      "command": ["dotnet", "fsi", "<SCRIPT>"],
      "change_directory": true
    }
  },
  "by_year": {
    "2020": {
      "lang": "fsharp",
      "separator": "-"
    },
    "2021": {
      "lang": "erlang"
    },
    "2022": {
      "lang": "ruby"
    }
  }
}
